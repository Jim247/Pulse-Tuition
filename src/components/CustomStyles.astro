---
import '@fontsource-variable/inter';
import '@fontsource/poppins';


// 'DM Sans'
// Nunito
// Dosis
// Outfit
// Roboto
// Literata
// 'IBM Plex Sans'
// Karla
// Poppins
// 'Fira Sans'
// 'Libre Franklin'
// Inconsolata
// Raleway
// Oswald
// 'Space Grotesk'
// Urbanist
---

<style is:inline>
  :root {
    --aw-font-sans: 'Cabin', sans-serif; 
    --aw-font-serif: 'Cabin', sans-serif; 
    --aw-font-heading: 'Cabin', sans-serif; 

    --aw-color-primary: black;
    --aw-color-secondary: #075985.;
    --aw-color-accent:  #075985.;



    --aw-color-text-heading:  rgb(31, 31, 31);
    --aw-color-text-default: rgb(31, 31, 31);
    --aw-color-text-muted: rgb(31, 31, 31);
    --aw-color-bg-page: rgb(253, 253, 253 );

    --aw-color-footer-bg: #075985;

    ::selection {
      background-color: lavender;
    }
  }

  .dark     {
    --aw-font-sans: 'Cabin', sans-serif; 
    --aw-font-serif: 'Cabin', sans-serif; 
    --aw-font-heading: 'Cabin', sans-serif; 

    --aw-color-primary: black;
    --aw-color-secondary: #075985.;
    --aw-color-accent:  #075985.;



    --aw-color-text-heading:  rgb(31, 31, 31);
    --aw-color-text-default: rgb(31, 31, 31);
    --aw-color-text-muted: rgb(31, 31, 31);
    --aw-color-bg-page: rgb(253, 253, 253 );

    --aw-color-footer-bg: #075985;
  }
    ::selection {
      background-color: lavender;
    }
</style>








